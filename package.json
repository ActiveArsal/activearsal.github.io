{
  "name": "activearsal.github.io",
  "version": "1.0.0",
  "description": "portfolio site",
  "main": "index.html",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.comp.css css/style.css --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ActiveArsal/ActiveArsal.github.io.git"
  },
  "author": "Arsal Mehran",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^7.0.0",
    "postcss-cli": "^4.1.1"
  },
  "bugs": {
    "url": "https://github.com/ActiveArsal/ActiveArsal.github.io/issues"
  },
  "homepage": "https://github.com/ActiveArsal/ActiveArsal.github.io#readme"
}
